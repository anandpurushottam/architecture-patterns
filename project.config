[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Anand (anandpurushottam)
[access "refs/for/refs/*"]
	read = group user/Anand (anandpurushottam)
	push = group user/Anand (anandpurushottam)
	label-Code-Review = -2..+2 group user/Anand (anandpurushottam)
	label-Verified = -1..+1 group user/Anand (anandpurushottam)
	rebase = group user/Anand (anandpurushottam)
	removeReviewer = group user/Anand (anandpurushottam)
	submit = group user/Anand (anandpurushottam)
[access "refs/heads/*"]
	read = group user/Anand (anandpurushottam)
	create = group user/Anand (anandpurushottam)
	push = +force group user/Anand (anandpurushottam)
	pushMerge = group user/Anand (anandpurushottam)
[access "refs/tags/*"]
	push = group user/Anand (anandpurushottam)
	push = +force group user/Anand (anandpurushottam)
